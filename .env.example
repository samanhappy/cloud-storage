# Cloud Storage Backend Configuration
# Choose one: aws-s3, qiniu, alibaba-oss
CLOUD_STORAGE_BACKEND=aws-s3

# AWS S3 Configuration
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
AWS_REGION=us-east-1
AWS_S3_BUCKET=your-s3-bucket-name
AWS_S3_ENDPOINT=https://s3.amazonaws.com

# Qiniu Cloud Storage Configuration
QINIU_ACCESS_KEY=your-qiniu-access-key
QINIU_SECRET_KEY=your-qiniu-secret-key
QINIU_BUCKET=your-qiniu-bucket-name
QINIU_DOMAIN=https://your-qiniu-domain.com
QINIU_ZONE=z0

# Alibaba Cloud OSS Configuration
ALIBABA_ACCESS_KEY_ID=your-alibaba-access-key-id
ALIBABA_ACCESS_KEY_SECRET=your-alibaba-access-key-secret
ALIBABA_OSS_BUCKET=your-alibaba-oss-bucket-name
ALIBABA_OSS_REGION=oss-cn-beijing
ALIBABA_OSS_ENDPOINT=https://oss-cn-beijing.aliyuncs.com

# Optional Configuration
MAX_FILE_SIZE=10485760
ALLOWED_MIME_TYPES=image/jpeg,image/png,image/gif,application/pdf,text/plain
URL_PREFIX=https://cdn.yourdomain.com/
CONFIG_FILE=./config.json
