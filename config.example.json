{
  "backend": {
    "type": "aws-s3",
    "accessKeyId": "your-aws-access-key-id",
    "secretAccessKey": "your-aws-secret-access-key",
    "region": "us-east-1",
    "bucket": "your-s3-bucket-name",
    "prefix": "my-app",
    "cdn": "https://cdn.yourdomain.com"
  },
  "maxFileSize": 10485760,
  "allowedMimeTypes": [
    "image/jpeg",
    "image/png",
    "image/gif",
    "image/webp",
    "application/pdf",
    "text/plain"
  ],
  "urlPrefix": "https://cdn.yourdomain.com/",
  "expirationTime": 3600
}
